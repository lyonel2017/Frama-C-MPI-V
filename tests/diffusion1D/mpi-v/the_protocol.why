module MPI_the_protocol

use protocol.MPI_Protocol
use int.Int
use int.EuclideanDivision

let constant the_protocol : protocol =
    (Seq (Broadcast 0 1 MPI_int)
    (Seq (Broadcast 0 1 MPI_int)
    (Seq (Broadcast 0 1 MPI_int)
         (Foreach 1 (size-1) (fun i -> Message 0 i (div 10000 size) 0 MPI_float)))))



end
